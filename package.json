{
  "name": "community-packing-list",
  "version": "1.0.0",
  "description": "A Django-based web application for creating, sharing, and managing structured packing lists",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack --mode development --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "css:build": "postcss src/styles/main.css -o packing_lists/static/packing_lists/css/compiled.css",
    "css:dev": "postcss src/styles/main.css -o packing_lists/static/packing_lists/css/compiled.css --watch",
    "format": "prettier --write src/**/*.{ts,css}",
    "test": "jest",
    "clean": "rimraf packing_lists/static/packing_lists/js/* packing_lists/static/packing_lists/css/compiled.css"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.21",
    "css-loader": "^6.8.0",
    "cssnano": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^30.0.5",
    "postcss": "^8.5.6",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "postcss-nested": "^6.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "style-loader": "^3.3.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.0.0",
    "typescript": "^5.2.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "axios": "^1.5.0",
    "intersection-observer": "^0.12.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
